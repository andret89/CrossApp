<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CrossApp.App">
    <Application.Resources>
        <OnPlatform x:TypeArguments="x:String" x:Key="SolidFont">
            <On Platform="Android" Value="fa-solid-900.ttf#fa-solid-900" />
            <On Platform="UWP" Value="/Assets/fa-solid-900.ttf#fa-solid-900" />
            <On Platform="iOS" Value="fa-solid-900" />
        </OnPlatform>
        <OnPlatform x:TypeArguments="x:String" x:Key="NormalFont">
            <On Platform="Android" Value="fa-regular-400.ttf#fa-regular-400" />
            <On Platform="UWP" Value="/Assets/fa-regular-400.ttf#fa-regular-400" />
            <On Platform="iOS" Value="fa-regular-400" />
        </OnPlatform>
    </Application.Resources>
</Application>